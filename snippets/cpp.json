{
	"cout-variable": {
		"prefix": "coutv",
		"body": [
			"std::cout << ${1:var} << std::endl;\n$0"
		],
		"description": "Output variable"
	},
	"add-endl": {
		"prefix": "endl",
		"body": [
			"std::cout << std::endl;\n"
		],
		"description": "Output endl"
	},
	"multiline-comment": {
		"prefix": "mcom",
		"body": [
			"/*${1}\n */\n$0"
		],
		"description": "Multiline comments"
	},
	"doc-comment": {
		"prefix": "dcom",
		"body": [
			"/* ${1:desc}\n * \n * @param ${2:param} ${3:desc}\n * \n * @return ${4:return}\n */$0"
		],
		"description": "Document comments"
	}
}
