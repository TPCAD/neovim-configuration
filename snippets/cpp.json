{
	"cout-variable": {
		"prefix": "coutv",
		"body": [
			"std::cout << ${1:var} << std::endl;\n$0"
		],
		"description": "Output variable"
	},
	"cout-variable-without-endl": {
		"prefix": "coutvw",
		"body": [
			"std::cout << ${1:var};\n$0"
		],
		"description": "Output variable without endl"
	},
	"add-endl": {
		"prefix": "endl",
		"body": [
			"std::cout << std::endl;\n"
		],
		"description": "Output endl"
	}
}
