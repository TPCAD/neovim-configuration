{
	"return-multiple-table": {
		"prefix": "retm",
		"body": [
			"local M = {}\n",
			"M.${1:tableName} = {\n",
			"${2:content}\n",
			"}\n",
			"return M\n$0"
		],
		"description": "Create and return multiple table"
	},
	"return-single-table": {
		"prefix": "ret",
		"body": [
			"return {\n",
			"${1:content}\n",
			"}\n$0"
		],
		"description": "Create and return a table"
	}
}
