{
	"multiline-comment": {
		"prefix": "mcom",
		"body": [
			"/*${1}\n */\n$0"
		],
		"description": "Multiline comments"
	},
	"doc-comment": {
		"prefix": "dcom",
		"body": [
			"/**\n *  @brief  ${1:brief}\n *  @param  ${2:param}  ${3:desc}\n *  @return  ${4:return}\n */$0"
		],

		"description": "Document comments"
	}
}
